#include <iostream>
#include <vector>

bool is_palindromic(int x) {
    std::vector<int> digits;
    int length = 0;
    while (x != 0) {
        digits.push_back(x % 10);
        x = x / 10;
        length += 1;
    }
    for (int i = 0; i < length / 2; ++i) {
        if (digits[i] != digits[length - 1 - i]) {
            return 0;
        }
    }
    return 1;
}

int main()
{
        int x = 998001;
        for (int i = 0; x - i > 10000; ++i) {
            x = x - 1;
            if (is_palindromic(x)) {
                for (int k = 999; k > 99; --k) {
                    if ((x % k == 0) && (x / k > 100) && (x / k < 999)) {
                        std::cout << x << std::endl;
                        return (x);
                    }
                }
            }
        }
  
}


