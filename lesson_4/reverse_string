#include <iostream>

void reverse_str(char* pa) {
	 char* start = pa;
	 char* end = pa;
	while (	*end != '\0') {
		++end;
	}
	--end;
	while (start < end) {
		char tmp = *start;
		*start = *end;
		*end = tmp;
		++start;
		--end;
		}
}

int main()
{	
	char ch[] = "123456789\0";
	reverse_str(ch);
	int i = 0;
	while (ch[i] != '\0') {
		std::cout << ch[i] << std::endl;
		++i;
	}
}
