char* URLify(char*& str, int strlength) {
	int count_of_space = 0;
	for (int i = 0; i < strlength; ++i) {
		count_of_space += (str[i] == ' ');
	}
	char* ptr = str + strlength - 1;
	char* end = str + strlength + 2 * count_of_space - 1;
	while (ptr != str) {
		if (*ptr != ' ') {
			*end-- = *ptr--;
		}
		else {
			*end-- = '0';
			*end-- = '2';
			*end-- = '%';
			--ptr;
		}
	}
	return str ;
}
