#include <iostream>
#include <string>
using namespace std;

int main()
{
    static string miavor = "";
    static string tasnavor = "";
    static string haryuravor = "";
    static string hazaravor = "";
    int tvanshan_miavor;
    int tvanshan_tasnavor;
    int tvanshan_haryuravor;
    int tvanshan_hazaravor;
    string str = "4";
    if (str.size() == 4) {
        tvanshan_miavor = str[3] - 48;
        tvanshan_tasnavor = str[2] - 48;
        tvanshan_haryuravor = str[1] - 48;
        tvanshan_hazaravor = str[0] - 48;
        if (tvanshan_miavor > 0 && tvanshan_miavor <= 3) {
            for (int i = 1; i <= tvanshan_miavor; ++i) {
                miavor = miavor + "I";
            }
        }
        if (tvanshan_miavor == 4) {
            miavor = miavor + "IV";
        }
        if (tvanshan_miavor < 9 && tvanshan_miavor >4) {
            miavor = "V";
            for (int i = 6; i <= tvanshan_miavor; ++i) {
                miavor = miavor + "I";
            }
        }
        if (tvanshan_miavor == 9) {
            miavor = "IX";
        }
        if (tvanshan_tasnavor <= 3) {
            for (int i = 1; i <= tvanshan_tasnavor; ++i) {
                tasnavor = tasnavor + "X";
            }
        }
        if (tvanshan_tasnavor == 4) {
            tasnavor = tasnavor + "XL";
        }
        if (tvanshan_tasnavor < 9 && tvanshan_tasnavor >4) {
            tasnavor = "L";
            for (int i = 6; i <= tvanshan_tasnavor; ++i) {
                tasnavor = tasnavor + "X";
            }
        }
        if (tvanshan_tasnavor == 9) {
            tasnavor = tasnavor + "XC";
        }
        if (tvanshan_haryuravor <= 3) {
            for (int i = 1; i <= tvanshan_haryuravor; ++i) {
                haryuravor = haryuravor + "C";
            }
        }
        if (tvanshan_haryuravor == 4) {
            haryuravor = haryuravor + "CD";
        }
        if (tvanshan_haryuravor < 9 && tvanshan_haryuravor >4) {
            haryuravor = "D";
            for (int i = 6; i <= tvanshan_haryuravor; ++i) {
                haryuravor = haryuravor + "C";
            }
        }
        if (tvanshan_haryuravor == 9) {
            haryuravor = haryuravor + "CM";
        }
        if (tvanshan_hazaravor <= 3) {
            for (int i = 1; i <= tvanshan_hazaravor; ++i) {
                hazaravor = hazaravor + "M";
            }
        }
    }
    else if (str.size() == 3) {

        tvanshan_miavor = str[2] - 48;
        tvanshan_tasnavor = str[1] - 48;
        tvanshan_haryuravor = str[0] - 48;
        if (tvanshan_miavor > 0 && tvanshan_miavor <= 3) {
            for (int i = 1; i <= tvanshan_miavor; ++i) {
                miavor = miavor + "I";
            }
        }
        if (tvanshan_miavor == 4) {
            miavor = miavor + "IV";
        }
        if (tvanshan_miavor < 9 && tvanshan_miavor >4) {
            miavor = "V";
            for (int i = 6; i <= tvanshan_miavor; ++i) {
                miavor = miavor + "I";
            }
        }
        if (tvanshan_miavor == 9) {
            miavor = "IX";
        }
        if (tvanshan_tasnavor <= 3) {
            for (int i = 1; i <= tvanshan_tasnavor; ++i) {
                tasnavor = tasnavor + "X";
            }
        }
        if (tvanshan_tasnavor == 4) {
            tasnavor = tasnavor + "XL";
        }
        if (tvanshan_tasnavor < 9 && tvanshan_tasnavor >4) {
            tasnavor = "L";
            for (int i = 6; i <= tvanshan_tasnavor; ++i) {
                tasnavor = tasnavor + "X";
            }
        }
        if (tvanshan_tasnavor == 9) {
            tasnavor = tasnavor + "XC";
        }
        if (tvanshan_haryuravor <= 3) {
            for (int i = 1; i <= tvanshan_haryuravor; ++i) {
                haryuravor = haryuravor + "C";
            }
        }
        if (tvanshan_haryuravor == 4) {
            haryuravor = haryuravor + "CD";
        }
        if (tvanshan_haryuravor < 9 && tvanshan_haryuravor >4) {
            haryuravor = "D";
            for (int i = 6; i <= tvanshan_haryuravor; ++i) {
                haryuravor = haryuravor + "C";
            }
        }
        if (tvanshan_haryuravor == 9) {
            haryuravor = haryuravor + "CM";
        }
    }
    else if (str.size() == 2) {
        tvanshan_miavor = str[1] - 48;
        tvanshan_tasnavor = str[0] - 48;
        if (tvanshan_miavor > 0 && tvanshan_miavor <= 3) {
            for (int i = 1; i <= tvanshan_miavor; ++i) {
                miavor = miavor + "I";
            }
        }
        if (tvanshan_miavor == 4) {
            miavor = miavor + "IV";
        }
        if (tvanshan_miavor < 9 && tvanshan_miavor >4) {
            miavor = "V";
            for (int i = 6; i <= tvanshan_miavor; ++i) {
                miavor = miavor + "I";
            }
        }
        if (tvanshan_miavor == 9) {
            miavor = "IX";
        }
        if (tvanshan_tasnavor <= 3) {
            for (int i = 1; i <= tvanshan_tasnavor; ++i) {
                tasnavor = tasnavor + "X";
            }
        }
        if (tvanshan_tasnavor == 4) {
            tasnavor = tasnavor + "XL";
        }
        if (tvanshan_tasnavor < 9 && tvanshan_tasnavor >4) {
            tasnavor = "L";
            for (int i = 6; i <= tvanshan_tasnavor; ++i) {
                tasnavor = tasnavor + "X";
            }
        }
        if (tvanshan_tasnavor == 9) {
            tasnavor = tasnavor + "XC";
        }
    }
    else {
        tvanshan_miavor = str[0] - 48;
        if (tvanshan_miavor > 0 && tvanshan_miavor <= 3) {
            for (int i = 1; i <= tvanshan_miavor; ++i) {
                miavor = miavor + "I";
            }
        }
        if (tvanshan_miavor == 4) {
            miavor = miavor + "IV";
        }
        if (tvanshan_miavor < 9 && tvanshan_miavor >4) {
            miavor = "V";
            for (int i = 6; i <= tvanshan_miavor; ++i) {
                miavor = miavor + "I";
            }
        }
        if (tvanshan_miavor == 9) {
            miavor = "IX";
        }
    }
    str = hazaravor + haryuravor + tasnavor + miavor;
    cout << str << endl;
}

