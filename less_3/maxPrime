#include <iostream>

using namespace std;

bool is_prime(int64_t x){
      for(int64_t i=2; i<=x/2; ++i){
          if(x%i==0){
              return false;
          }
      }
      return true;
}

int main()
{
    int64_t max;
    int64_t x = 600851475143 ;
    for (int64_t i=x/2; i>= 2; --i){
        if(x%i == 0 && is_prime(i)){
            max = i;
            break;
        }
    }
    cout<<max<<endl;
}
